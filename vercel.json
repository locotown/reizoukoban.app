{
  "version": 2,
  "rewrites": [
    {
      "source": "/app",
      "destination": "/build/web/index.html"
    },
    {
      "source": "/app/(.*)",
      "destination": "/build/web/$1"
    },
    {
      "source": "/",
      "destination": "/landing/index.html"
    },
    {
      "source": "/css/(.*)",
      "destination": "/landing/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/landing/js/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/landing/assets/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ]
}
